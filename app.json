{
  "expo": {
    "name": "Swiss Pro",
    "slug": "cryptowallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMotionUsageDescription": "This app uses the accelerometer to detect motion and gestures.",
        "NSFaceIDUsageDescription": "This app uses Face ID for authentication."
      },
      "bundleIdentifier": "com.codewithhonour.cryptowallet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.codewithhonour.cryptowallet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-vision-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "9e040e95-b71a-434a-af24-04fe1d958ee6"
      }
    }
  }
}
